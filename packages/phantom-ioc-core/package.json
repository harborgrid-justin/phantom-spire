{
  "name": "@phantom-spire/ioc-core",
  "version": "1.0.1",
  "description": "Enterprise IOC (Indicators of Compromise) processing core with advanced threat correlation and ML-driven analysis",
  "main": "index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/harborgrid-justin/phantom-spire#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/harborgrid-justin/phantom-spire.git",
    "directory": "packages/phantom-ioc-core"
  },
  "bugs": {
    "url": "https://github.com/harborgrid-justin/phantom-spire/issues"
  },
  "napi": {
    "name": "phantom-ioc-core",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-pc-windows-msvc",
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --features napi",
    "build:debug": "napi build --platform --features napi",
    "build:enterprise": "napi build --platform --release --features enterprise",
    "build:full": "napi build --platform --release --features full",
    "clean": "rimraf dist",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "cargo test --features enterprise",
    "test:native": "cargo test --all-features",
    "universal": "napi universal",
    "version": "napi version",
    "lint": "cargo clippy --features enterprise",
    "fmt": "cargo fmt"
  },
  "keywords": [
    "@phantom-spire",
    "enterprise-cti",
    "ioc",
    "indicators",
    "threat-intelligence",
    "cybersecurity",
    "phantom-spire",
    "napi",
    "rust",
    "security"
  ],
  "author": "Phantom Spire Security Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "rimraf": "^6.0.1"
  },
  "optionalDependencies": {
    "@phantom-spire/ioc-core-win32-x64-msvc": "1.0.1",
    "@phantom-spire/ioc-core-darwin-x64": "1.0.1",
    "@phantom-spire/ioc-core-darwin-arm64": "1.0.1",
    "@phantom-spire/ioc-core-linux-x64-gnu": "1.0.1",
    "@phantom-spire/ioc-core-linux-arm64-gnu": "1.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "organization": {
    "name": "Phantom Spire",
    "url": "https://github.com/harborgrid-justin/phantom-spire"
  },
  "dependencies": {
    "@phantom-spire/ml-core": "file:../phantom-ml-core/nextgen"
  }
}
