{
  "name": "@phantom-spire/ml-core",
  "version": "1.0.1",
  "description": "Enterprise machine learning services for threat detection and security analytics - Part of the Phantom Spire CTI Platform",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/harborgrid-justin/phantom-spire.git",
    "directory": "packages/phantom-ml-core"
  },
  "license": "MIT",
  "browser": "browser.js",
  "keywords": [
    "@phantom-spire",
    "enterprise-cti",
    "machine-learning",
    "cybersecurity",
    "threat-detection",
    "anomaly-detection",
    "napi-rs",
    "rust",
    "enterprise",
    "security-analytics"
  ],
  "files": [
    "index.d.ts",
    "index.js",
    "browser.js",
    "*.node"
  ],
  "napi": {
    "binaryName": "phantom-ml-core",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "Phantom Spire Security Team",
  "organization": {
    "name": "Phantom Spire",
    "url": "https://github.com/harborgrid-justin/phantom-spire"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "bench": "node --import @oxc-node/core/register benchmark/bench.ts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "build:dev": "napi build --platform",
    "dev": "napi build --platform && node simple-test.js",
    "format": "run-p format:prettier format:rs format:toml",
    "format:prettier": "prettier . -w",
    "format:toml": "taplo format",
    "format:rs": "cargo fmt",
    "lint": "oxlint .",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "test:native": "cargo test",
    "universal": "napi universal",
    "preversion": "napi build --platform --release && git add .",
    "version": "napi version",
    "prepare": "husky"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@oxc-node/core": "^0.0.32",
    "@taplo/cli": "^0.7.0",
    "ava": "^6.4.1",
    "chalk": "^5.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.14.0",
    "prettier": "^3.6.2",
    "tinybench": "^5.0.1",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "oxlint --fix"
    ],
    "*.@(js|ts|tsx|yml|yaml|md|json)": [
      "prettier --write"
    ],
    "*.toml": [
      "taplo format"
    ]
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "timeout": "2m",
    "workerThreads": false,
    "environmentVariables": {
      "OXC_TSCONFIG_PATH": "./__test__/tsconfig.json"
    },
    "nodeArguments": [
      "--import",
      "@oxc-node/core/register"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "packageManager": "yarn@4.9.4",
  "optionalDependencies": {
    "@phantom-spire/ml-core-android-arm64": "1.0.1",
    "@phantom-spire/ml-core-android-arm-eabi": "1.0.1",
    "@phantom-spire/ml-core-win32-x64-msvc": "1.0.1",
    "@phantom-spire/ml-core-win32-ia32-msvc": "1.0.1",
    "@phantom-spire/ml-core-win32-arm64-msvc": "1.0.1",
    "@phantom-spire/ml-core-darwin-universal": "1.0.1",
    "@phantom-spire/ml-core-darwin-x64": "1.0.1",
    "@phantom-spire/ml-core-darwin-arm64": "1.0.1",
    "@phantom-spire/ml-core-freebsd-x64": "1.0.1",
    "@phantom-spire/ml-core-freebsd-arm64": "1.0.1",
    "@phantom-spire/ml-core-linux-x64-musl": "1.0.1",
    "@phantom-spire/ml-core-linux-x64-gnu": "1.0.1",
    "@phantom-spire/ml-core-linux-arm64-musl": "1.0.1",
    "@phantom-spire/ml-core-linux-arm64-gnu": "1.0.1",
    "@phantom-spire/ml-core-linux-arm-musleabihf": "1.0.1",
    "@phantom-spire/ml-core-linux-arm-gnueabihf": "1.0.1",
    "@phantom-spire/ml-core-linux-loong64-musl": "1.0.1",
    "@phantom-spire/ml-core-linux-loong64-gnu": "1.0.1",
    "@phantom-spire/ml-core-linux-riscv64-musl": "1.0.1",
    "@phantom-spire/ml-core-linux-riscv64-gnu": "1.0.1",
    "@phantom-spire/ml-core-linux-ppc64-gnu": "1.0.1",
    "@phantom-spire/ml-core-linux-s390x-gnu": "1.0.1",
    "@phantom-spire/ml-core-openharmony-arm64": "1.0.1",
    "@phantom-spire/ml-core-openharmony-x64": "1.0.1",
    "@phantom-spire/ml-core-openharmony-arm": "1.0.1",
    "@phantom-spire/ml-core-wasm32-wasi": "1.0.1"
  }
}
