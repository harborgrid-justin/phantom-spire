{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "../",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "../src/models/HuggingFaceModelBase.ts",
    "../src/lib/ml-core/index.ts",
    "../src/app/api/model-builder/route.ts",
    "../src/utils/lazyLoading.tsx"
  ]
}