  "scripts": {
    "dev": "next dev -p 3003",
    "dev:turbopack": "next dev --turbopack",
    "build": "next build",
    "build:turbopack": "next build --turbopack",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "eslint",
    "lint:boundary": "node scripts/check-boundaries.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:report": "cypress run --reporter mochawesome",
    "test:e2e": "cypress run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:e2e:report": "start-server-and-test dev http://localhost:3000 \"cypress run --reporter mochawesome\"",
    "test:report:merge": "mochawesome-merge cypress/results/mochawesome/*.json > cypress/results/merged-report.json",
    "test:report:generate": "marge cypress/results/merged-report.json --reportDir cypress/results/html --inline",
    "audit:nextjs": "npm run lint:boundary && npm run build:analyze",
    "nextjs:check-updates": "node scripts/nextjs-upgrade-checklist.js check",
    "nextjs:upgrade-update": "node scripts/nextjs-upgrade-checklist.js update",
    "nextjs:upgrade-cleanup": "node scripts/nextjs-upgrade-checklist.js cleanup",
    "nextjs:upgrade-report": "node scripts/nextjs-upgrade-checklist.js report",
    "security:check": "node scripts/pre-commit-security-check.js",
    "env:validate": "node scripts/validate-env-setup.js",
    "sync-db": "tsx scripts/sync-database.ts",
    "sync-db:alter": "tsx scripts/sync-database.ts --alter",
    "sync-db:seed": "tsx scripts/sync-database.ts --seed",
    "sync-db:force": "tsx scripts/sync-database.ts --force",
    "sync-db:help": "tsx scripts/sync-database.ts --help",
    "sync-core": "tsx scripts/sync-core-models.ts",
    "sync-core:alter": "tsx scripts/sync-core-models.ts --alter",
    "sync-core:force": "tsx scripts/sync-core-models.ts --force",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
