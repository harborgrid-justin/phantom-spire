{
  "name": "@phantom-core/threat-actor",
  "version": "1.0.16",
  "description": "Enterprise-Grade Threat Intelligence & Attribution Platform - Advanced threat actor analysis with 94.2% attribution accuracy, OCSF compliance, and real-time security operations support",
  "main": "index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/defendr-ai/phantom.core-threat-actor#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/defendr-ai/phantom.core-threat-actor.git"
  },
  "bugs": {
    "url": "https://github.com/defendr-ai/phantom.core-threat-actor/issues"
  },
  "napi": {
    "binaryName": "phantom-core-threat-actor",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-pc-windows-gnu",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu"
    ]
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "API_DOCUMENTATION.md",
    "ENTERPRISE_GUIDE.md"
  ],
  "scripts": {
    "artifacts": "npx @napi-rs/cli artifacts",
    "build": "npx @napi-rs/cli build --platform --release --features napi",
    "build:debug": "npx @napi-rs/cli build --platform --features napi",
    "build:enterprise": "npx @napi-rs/cli build --platform --release --features enterprise",
    "build:full": "npx @napi-rs/cli build --platform --release --features full",
    "clean": "npx rimraf dist",
    "prepublishOnly": "echo 'Skipping prepublishOnly - handled by CI/CD'",
    "test": "cargo test --features enterprise",
    "test:native": "cargo test --all-features",
    "universal": "npx @napi-rs/cli universal",
    "lint": "cargo clippy --features enterprise",
    "fmt": "cargo fmt"
  },
  "keywords": [
    "enterprise-threat-intelligence",
    "threat-actor-attribution",
    "security-operations-center",
    "apt-analysis",
    "campaign-tracking",
    "behavioral-analysis",
    "ocsf-compliance",
    "threat-hunting",
    "cybersecurity",
    "enterprise-security",
    "threat-attribution",
    "security-analytics",
    "incident-response",
    "threat-intelligence",
    "phantom-spire",
    "enterprise-grade",
    "soc",
    "cti",
    "rust",
    "napi"
  ],
  "author": "Phantom Spire Security Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.2.0",
    "rimraf": "^5.0.10"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "organization": {
    "name": "Defendr AI",
    "url": "https://github.com/defendr-ai/phantom.core-threat-actor"
  },
  "optionalDependencies": {
    "@phantom-core/threat-actor-darwin-x64": "1.0.6",
    "@phantom-core/threat-actor-linux-x64-gnu": "1.0.6",
    "@phantom-core/threat-actor-win32-x64-msvc": "1.0.6",
    "@phantom-core/threat-actor-win32-x64-gnu": "1.0.6"
  }
}
