{
  "name": "phantom-attribution-core",
  "version": "1.0.1",
  "description": "Threat attribution and actor profiling core with advanced behavioral analysis and campaign tracking - Part of the Phantom Spire CTI Platform",
  "main": "attribution-wrapper.js",
  "types": "index.d.ts",
  "bin": {
    "phantom-attribution-example": "./example.js"
  },
  "homepage": "https://github.com/harborgrid-justin/phantom-spire#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/harborgrid-justin/phantom-spire.git",
    "directory": "packages/phantom-attribution-core"
  },
  "bugs": {
    "url": "https://github.com/harborgrid-justin/phantom-spire/issues"
  },
  "napi": {
    "name": "phantom-attribution-core",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-pc-windows-msvc",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "files": [
    "attribution-wrapper.js",
    "index.d.ts",
    "*.node",
    "example.js",
    "test-package.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "INSTALL.md"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --features napi",
    "build:debug": "napi build --platform --features napi",
    "build:enterprise": "napi build --platform --release --features enterprise",
    "build:full": "napi build --platform --release --features full",
    "clean": "rimraf dist target",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "cargo test",
    "test:native": "cargo test --all-features",
    "test:node": "node -e \"const pkg = require('./attribution-wrapper.js'); console.log('Package loads successfully:', typeof pkg.PhantomAttributionCore)\"",
    "test:package": "node test-package.js",
    "universal": "napi universal",
    "version": "napi version",
    "lint": "cargo clippy",
    "fmt": "cargo fmt",
    "check": "cargo check --all-features",
    "docs": "cargo doc --no-deps"
  },
  "keywords": [
    "phantom-attribution-core",
    "phantom-spire",
    "enterprise-cti",
    "attribution",
    "threat-actors",
    "profiling",
    "intelligence",
    "behavioral-analysis",
    "cybersecurity",
    "napi",
    "rust",
    "security",
    "threat-intelligence",
    "cti"
  ],
  "author": "Phantom Spire Security Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.2.0",
    "@types/node": "^20.0.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@node-rs/helper": "^1.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "organization": {
    "name": "Phantom Spire",
    "url": "https://github.com/harborgrid-justin/phantom-spire"
  }
}
