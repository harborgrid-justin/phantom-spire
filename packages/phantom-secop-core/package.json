{
  "name": "@phantom-spire/secop-core",
  "version": "1.0.1",
  "description": "Advanced Security Operations (SecOps) system for incident response, orchestration, automation, and operational intelligence with enterprise-grade capabilities",
  "main": "secop-wrapper.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/harborgrid-justin/phantom-spire#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/harborgrid-justin/phantom-spire.git",
    "directory": "packages/phantom-secop-core"
  },
  "bugs": {
    "url": "https://github.com/harborgrid-justin/phantom-spire/issues"
  },
  "napi": {
    "name": "phantom-secop-core",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-pc-windows-msvc",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "files": [
    "secop-wrapper.js",
    "phantom_secop_core.node",
    "phantom-secop-core.win32-x64-msvc.node",
    "phantom-secop-core.win32-x64-gnu.node",
    "index.d.ts",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --features napi",
    "build:debug": "napi build --platform --features napi",
    "build:enterprise": "napi build --platform --release --features enterprise",
    "build:full": "napi build --platform --release --features full",
    "clean": "rimraf dist",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "cargo test --features enterprise",
    "test:native": "cargo test --all-features",
    "universal": "napi universal",
    "version": "napi version",
    "lint": "cargo clippy --features enterprise",
    "fmt": "cargo fmt"
  },
  "keywords": [
    "@phantom-spire",
    "enterprise-cti",
    "secops",
    "incident-response",
    "security-automation",
    "security-orchestration",
    "cybersecurity",
    "phantom-spire",
    "napi",
    "rust",
    "security"
  ],
  "author": "Phantom Spire Security Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.2.0",
    "rimraf": "^6.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "organization": {
    "name": "Phantom Spire",
    "url": "https://github.com/harborgrid-justin/phantom-spire"
  },
  "dependencies": {
    "@phantom-spire/ml-core": "file:../phantom-ml-core/nextgen"
  }
}
