{
  "name": "phantom-secop-core",
  "version": "1.0.0",
  "description": "Advanced Security Operations (SecOps) engine for comprehensive incident response, automation, and security orchestration",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "phantom-secop-core",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-pc-windows-msvc",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "cargo test",
    "universal": "napi universal",
    "version": "napi version"
  },
  "keywords": [
    "security",
    "secops",
    "incident-response",
    "security-automation",
    "security-orchestration",
    "threat-detection",
    "security-analytics",
    "compliance",
    "evidence-management",
    "playbook-automation"
  ],
  "author": "Phantom Spire Security",
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">= 10"
  }
}
